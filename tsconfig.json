{
    "compilerOptions": {
        "declaration": true,
        "alwaysStrict": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "strictNullChecks": true,
        "target": "es5"
    },
    "include": [
        "source/**/*.ts"
    ],
    "exclude": [
        "source/**/*.d.ts"
    ]
}